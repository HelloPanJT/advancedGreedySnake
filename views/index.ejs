    <!-- Latest compiled and minified JavaScript -->
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>
</head>
<body>
	<div class="container">
		<header>
		</header>
    <div class="row">
      <div class="col-md-8">
    		<table id='qipan' cellpadding="0" cellspacing="0">
    		</table>
      </div>
      <div class="col-md-4">
        <div class="row">
          LeaderBoard:
          <table class="table">
            <tr>
              <td>Test:</td>
              <td>2</td>
            </tr>
            <tr>
              <td>Test:</td>
              <td>2</td>
            </tr>
            <tr>
              <td>Test:</td>
              <td>2</td>
            </tr>
            <tr>
              <td>Test:</td>
              <td>2</td>
            </tr>
            <tr>
              <td>Test:</td>
              <td>2</td>
            </tr>
          </table>
        </div>
        <div class="row">
          Status:
          <table class="table">
            <tr>
              <td>Username:</td>
              <td>Test</td>
            </tr>
            <tr>
              <td>Length:</td>
              <td>14</td>
            </tr>
          </table>
        </div>
        <div class="row">

          Chat Room:
          <div class="panel panel-default">
            <div class="panel-body">
              <p>AAA: as</p>
              <p>AAA: as</p>
              <p>AAA: as</p>
              <p>AAA: as</p>
              <p>AAA: as</p>
              <p>AAA: as</p>
            </div>
          </div>
          <form class="form-inline">
            <input type="text" class="form-control" placeholder="Text input">
            <button type="button" class="btn btn-info">Send</button>
          </form>
        </div>
      </div>
    </div>
		<div id='status'></div>
	</div>
	<script type="text/javascript">
		var size=20;
		var tb=$('#qipan');
		var tbContent="";
		for(var i=0;i<size;i++) {
			 tbContent+='<tr>';
			     for(var j=0;j<size;j++){
			    	   tbContent+='<td></td>';
			    	}
			 tbContent+='</tr>';
	     }
		tb.append(tbContent);
	</script>
</body>
</html>
